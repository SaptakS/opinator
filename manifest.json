{
	"name" : "Opinator",
	"version" : "1.0.0",
	"manifest_version":2,
	"description" : "Change background to yellow",
	"browser_action" : {
		"name" : "Yellow Extension",
		"default_icon": "icons/images19.png"
	},
	"background": {
		"page": "background.html"
	},
	"permissions" : ["tabs", "http://*/*"],
	"icons" : {
		"16": "icons/images16.png",
		"48": "icons/images48.png",
		"128": "icons/images128.png"
	}
}